{
  "name": "kushklicker-admin",
  "version": "1.0.0",
  "description": "KushKlicker Admin Panel",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --port 3001 --host 0.0.0.0",
    "dev:server": "tsx server/admin-server.ts",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3001",
    "start": "node server/admin-server.js",
    "start:server": "tsx server/admin-server.ts"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@tanstack/react-query": "^4.36.1",
    "wouter": "^3.3.5",
    "lucide-react": "^0.447.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "express": "^4.19.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@vitejs/plugin-react": "^4.3.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "tailwindcss": "^3.4.14",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tsx": "^4.7.1",
    "concurrently": "^8.2.2"
  }
}